<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=no">
    <title><%= current_page.data.title || "Jack Reynolds" %></title>

    <%= stylesheet_link_tag "application" %>
    <script src="//use.typekit.net/qja2nny.js"></script>
    <script>try{Typekit.load();}catch(e){}</script>
  </head>

  <body class="<%= page_classes %>">
    <%= yield %>

    <%= javascript_include_tag  "application" %>
    <script src="javascripts/vendor/jquery.min.js"></script>
    <script src="javascripts/vendor/underscore-min.js"></script>
    <script>$(function($){ site.initialize(); });</script>
  </body>
</html>